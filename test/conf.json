{
  "url": "https://faebeee.github.io/smurf-fetch/",
  "loaders": [
    "CSSStatsLoader",
    "CSSAnalyzeLoader",
    "PSILoader",
    "W3CLoader",
    "SEOLoader"
  ],
  "userConf": {
    "chunkSize": 3
  },
  "loaderConf": [
    {
      "key": "CSSStatsLoader",
      "config": {
        "timeout": 15000
      }
    },
    {
      "key": "CSSAnalyzeLoader",
      "config": {
        "timeout": 15000
      }
    },
    {
      "key": "PSILoader",
      "budget": {
        "ruleGroups": {
          "SPEED": {
            "score": 90
          }
        }
      },
      "config": {
        "strategy": "desktop"
      }
    },
    {
      "key": "W3CLoader",
      "config": {}
    },
    {
      "key": "SEOLoader",
      "config": {}
    }
  ]
}
